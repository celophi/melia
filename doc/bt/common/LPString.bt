// Length-prefixed string
typedef struct
{
    short length;
    if(length > 0)
        char text[length];
}
LPString <read=LPStringRead>;

string LPStringRead(LPString &v)
{
    if(v.length > 0)
        return v.text;
    else
        return "";
}